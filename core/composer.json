{
    "name": "young-developers/project-automatization",
    "description": "Core of Project Automatization",
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Bruna",
            "email": "mbrunas.p@gmail.com"
        }
    ],
    "require": {
        "php": "8.2"
    },
    "autoload": {
        "psr-4": {
            "ProjectAutomatization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.7",
        "squizlabs/php_codesniffer": "^3.7.2",
        "slevomat/coding-standard": "^8.8.0",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.0.17",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
